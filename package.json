{
  "name": "mic-recorder-to-mp3",
  "version": "1.0.0",
  "description": "Record your microphone audio input and get an audio/mp3 ouput buffer/blob.",
  "main": "dist/mic-recorder.js",
  "scripts": {
    "build": "node rollup.config.js"
  },
  "repository": "git@github.com:djalmaaraujo/mic-recorder-to-mp3.git",
  "author": "Djalma Araujo <djalma@close.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "dependencies": {
    "lamejs": "git://github.com/zhuker/lamejs.git#c4638f8f4e7801d415aaabaae29d305d75c1bd38"
  }
